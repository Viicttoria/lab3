@model IEnumerable<DeliverySite.Models.DeliveryMan>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DeliveryManName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContactNumberDeliveryMan)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmailDeliveryMan)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CommandId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DeliveryManName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ContactNumberDeliveryMan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmailDeliveryMan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CommandId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
